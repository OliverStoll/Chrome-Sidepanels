{
  "manifest_version": 3,
  "name": "Telegram",
  "version": "0.0.1",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+W",
        "mac": "Alt+W",
        "chromeos": "Alt+W",
        "linux": "Alt+W"
      }
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Telegram"
  },
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["sidePanel", "declarativeNetRequest"]
}
